            <Label text="Board title:" textAlignment="JUSTIFY">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="boardTitle" />
            <HBox prefHeight="111.0" prefWidth="326.0" spacing="200.0">
